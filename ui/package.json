{
  "name": "usb-writer-ui",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-container": "tsc && vite build && docker build -t webserver .",
    "preview": "vite preview",
    "start": "docker run -it --rm -d -p 8080:80 --name web webserver",
    "stop": "docker stop web"
  },
  "dependencies": {
    "immer": "^9.0.18",
    "preact": "^10.11.3"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.4.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
